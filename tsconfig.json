{
	"compilerOptions": {
		"target": "ES2021",
		"module": "ES2022",
		"esModuleInterop": false,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "node",
		"declaration": false,
		"lib": ["ES2021", "DOM"]
	},
	"include": [
		"*.ts",
		"c3runtime/*.ts",
		"ts-defs/editor/**/*.d.ts",
		"ts-defs/runtime/**/*.d.ts"
	]
}
